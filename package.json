{
  "name": "monad-11",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node ./node_modules/vite/bin/vite.js --host",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "compile:contracts": "cd contracts && npm install && npm run compile",
    "deploy": "cd contracts && npm run deploy && node ../scripts/sync-addresses.cjs",
    "deploy:seed": "cd contracts && npm run deploy && npm run seed && node ../scripts/sync-addresses.cjs",
    "backend": "cd backend && npm install && npm run dev",
    "dev:all": "npm run backend & npm run dev"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@tanstack/react-query": "^5.90.21",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "viem": "^2.46.1",
    "wagmi": "^2.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.4.0",
    "tailwindcss": "3.4.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "5.4.11"
  },
  "overrides": {
    "tailwindcss": "3.4.16"
  }
}
